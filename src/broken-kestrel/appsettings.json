{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    // change this to "True" to have the site break at startup
    "BreakEarly": "False",
    // change this to "True" to have the site break 10s after startup
    "BreakLater": "False",
    "BreakLaterDelaySeconds": 10
  },
  "AllowedHosts": "*"
}
